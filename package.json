    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTestFramework.js",
      "jest-localstorage-mock"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
